{
  "name": "primifydb",
  "version": "0.2.0",
  "description": "Really primitive Database for very specific UUID oriented / datetime oriented data fetching ",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "test": "npm run test",
    "dev": "nodemon --ignore 'databases/*' --exec babel-node --presets env,stage-2 ./src/index.js",
    "start": "babel-node --presets env,stage-2 ./src/index.js",
    "prod": "node dist/index.js",
    "build": "babel src -d dist --presets es2015,stage-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Everscape-Labs/PrimifyDB.git"
  },
  "keywords": [
    "database",
    "filesystem"
  ],
  "author": "Johan Leche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Everscape-Labs/PrimifyDB/issues"
  },
  "homepage": "https://github.com/Everscape-Labs/PrimifyDB#readme",
  "dependencies": {
    "fastify": "^1.9.0",
    "mkdirp": "^0.5.1",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
